      <valve duration = "600">Push Syringe</valve>
    </protocol>
    <protocol name = "Incubate 15min">
      <valve duration = "900">Push Syringe</valve>
    </protocol>

    <protocol name = "Flow Common Readouts">
      <valve duration = "3">Set Common Readouts</valve>
