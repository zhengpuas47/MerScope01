     </pump_cmd>
     <pump_cmd name = "Empty Syringe">
       <pump_config position = "0" speed = "33" direction = "Bypass"></pump_config>
     </pump_cmd>
     <pump_cmd name = "Take 0.1mL">
       <pump_config position = "100" speed = "33" direction = "Input"></pump_config>
     </pump_cmd>
     <pump_cmd name = "Take 0.5mL">
       <pump_config position = "500" speed = "33" direction = "Input"></pump_config>
     </pump_cmd>
