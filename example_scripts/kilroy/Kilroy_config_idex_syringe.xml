       <pump_config position = "1000" speed = "100" direction = "Input"></pump_config>
     </pump_cmd>
     <pump_cmd name = "Fast Push Syringe">
       <pump_config position = "0" speed = "100" direction = "Output"></pump_config>
     </pump_cmd>
     <pump_cmd name = "Fast Empty Syringe">
